<view class="content"><view class="input-search"><view class="input-box"><input type="text" data-event-opts="{{[['input',[['__set_model',['','keywordinput','$event',[]]]]]]}}" value="{{keywordinput}}" bindinput="__e"/><view class="iconfont" style="position:fixed;margin-top:-56rpx;left:60rpx;z-index:1000;width:40rpx;height:40rpx;"><image style="width:100%;height:100%;" src="../../static/img/sousuo.png"></image></view></view><button class="btn" type="primary" data-event-opts="{{[['tap',[['search']]]]}}" bindtap="__e">搜索</button></view><view style="height:120rpx;"></view><view class="uni-list"><block wx:for="{{itemList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="uni-list-cell" hover-class="uni-list-cell-hover"><view class="uni-media-list" data-id="{{item.id}}" data-event-opts="{{[['tap',[['openinfo',['$event']]]]]}}" bindtap="__e"><image class="img" src="{{item.userInfo.avatar}}" mode="aspectFill"></image><view class="uni-media-list-body"><view class="uni-media-list-text-top name">{{item.name+"（"+item.position+"）"}}</view><view class="uni-media-list-text-top company">{{item.company+" （"+item.composition+"）"}}</view></view></view><block wx:if="{{group_id==2}}"><view class="uni-media-list-text-top mobile" data-ph="{{item.mobile}}" data-event-opts="{{[['tap',[['callPhone',['$event']]]]]}}" bindtap="__e">{{item.mobile}}</view></block></view></block></view><uni-load-more vue-id="1" status="{{status}}" content-text="{{contentText}}" bind:__l="__l"></uni-load-more><view style="font-size:24rpx;color:#999;text-align:center;margin-bottom:30rpx;">杭州九之信息技术有限公司提供技术支持</view></view>